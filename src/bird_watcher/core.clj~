(ns bird-watcher.core
  (:gen-class))

(def last-week 
  [0 2 5 3 7 8 4])

(def birds-per-day
  [2 5 0 7 4 1])

(defn today [birds]
  (last birds)
  )

(defn inc-bird [birds]
  (inc (last birds-per-day)))

(defn day-without-birds? [birds]
  )

(defn n-days-count [birds n]
  )

(defn busy-days [birds]
  )

(defn odd-week? [birds]
  )
